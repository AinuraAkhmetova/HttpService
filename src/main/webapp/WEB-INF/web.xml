<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>WeatherServlet</servlet-name>
        <servlet-class>com.ibecsystems.servlets.WeatherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeZoneServlet</servlet-name>
        <servlet-class>com.ibecsystems.servlets.TimeZoneServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WeatherServlet</servlet-name>
        <url-pattern>/weather/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeZoneServlet</servlet-name>
        <url-pattern>/timezone</url-pattern>
    </servlet-mapping>
</web-app>
